<section>
    <div class="container">
        <table class="table mt-4">
  <thead>
    <tr>
      <th scope="col">No</th>
      <th scope="col">Date</th>
      <th scope="col">F Name</th>
      <th scope="col">L  Name</th>
      <th scope="col">Email </th>
      <th scope="col">Address</th>
      <th scope="col">Mobile</th>
      <th scope="col">Price</th>
      <th scope="col">Payment</th>
      <th scope="col">Status</th>
      <th scope="col">er</th>

      <th scope="col"></th>

    </tr>
  </thead>
  <tbody>
    {{#each getOrders}}
    <tr>
      <th scope="row">{{inc @index}}</th>
      <td>{{this.date}}</td>
      <td>{{this.deliveryDetails.firstname}}</td>
      <td>{{this.deliveryDetails.lastname}}</td>
      <td>{{this.deliveryDetails.email}}</td>
      <td>{{this.deliveryDetails.address}}</td>
      <td>{{this.deliveryDetails.Mobile}}</td>
      <td>&#8377;{{this.totalAmout}}</td>
      <td>{{this.PaymentMethod}}</td>
      <td>{{this.status}}</td>
      <td><a href="/order-details/{{this._id}}" class="btn btn-secondary">Order details</a></td>
         </tr>
    {{/each}}
  </tbody>
</table> 
